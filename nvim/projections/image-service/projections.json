{
  "application/*.php": {
    "alternate": "tests/phpunit/unit/Application/{}Test.php",
    "type": "source"
  },
  "application/controllers/*.php": {
    "alternate": "tests/phpunit/unit/Controllers/{}Test.php",
    "type": "source"
  },
  "library/*.php": {
    "alternate": "tests/phpunit/unit/Library/{}Test.php",
    "type": "source"
  },
  "library/Process/Driver/*.php": {
    "alternate": [
        "test/phpunit/unit/library/Driver/{}Test.php"
      ],
    "type": "source"
  },
  "test/phpunit/unit/library/Driver/*Test.php": {
    "alternate": [
        "library/Process/Driver/{}.php"
      ],
    "type": "source"
  },
  "tests/phpunit/unit/Library/*Test.php": {
    "alternate": "library/{}.php",
    "type": "test"
  },
  "tests/phpunit/unit/Application/*Test.php": {
    "alternate": "application/{}.php",
    "type": "test"
  },
  "tests/phpunit/unit/Controllers/*Test.php": {
    "alternate": "application/controllers/{}.php",
    "type": "test"
  }
}
