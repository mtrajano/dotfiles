{
  "application/*.php": {
    "alternate": "test/phpunit/unit/Application/{}Test.php",
    "type": "source"
  },
  "application/controllers/*.php": {
    "alternate": "test/phpunit/unit/Controllers/{}Test.php",
    "type": "source"
  },
  "library/*.php": {
    "alternate": "test/phpunit/unit/{}Test.php",
    "type": "source"
  },
  "library/Network/**/Commands/*.php": {
    "alternate": [
      "library/Network/{dirname}/Handlers/{basename}.php",
      "library/Network/{dirname}/CommandHandlers/{basename}.php",
      "test/phpunit/unit/Network/{dirname}/Commands/{basename}Test.php"
    ],
    "type": "command"
  },
  "library/Network/Command/Handlers/*.php": {
    "alternate": [
      "library/Network/Command/Commands/{}.php",
      "test/phpunit/unit/Network/Command/Handlers/{}Test.php"
    ],
    "type": "handler"
  },
  "library/Network/**/CommandHandlers/*.php": {
    "alternate": [
      "library/Network/{dirname}/Commands/{basename}.php",
      "test/phpunit/unit/Network/{dirname}/CommandHandlers/{basename}Test.php"
    ],
    "type": "handler"
  },
  "library/Network/**/Queries/*.php": {
    "alternate": [
      "library/Network/{dirname}/QueryHandlers/{basename}.php",
      "test/phpunit/unit/Network/{dirname}/Queries/{basename}Test.php"
    ],
    "type": "query"
  },
  "library/Network/**/QueryHandlers/*.php": {
    "alternate": [
      "library/Network/{dirname}/Queries/{basename}.php",
      "test/phpunit/unit/Network/{dirname}/QueryHandlers/{basename}Test.php"
    ],
    "type": "handler"
  },
  "test/phpunit/unit/Application/*Test.php": {
    "alternate": "application/{}.php",
    "type": "test"
  },
  "test/phpunit/unit/Controllers/*Test.php": {
    "alternate": "application/controllers/{}.php",
    "type": "test"
  },
  "test/phpunit/unit/*Test.php": {
    "alternate": "library/{}.php",
    "type": "test"
  }
}
